source 'https://rubygems.org'

gem 'rails', '3.2.3'

gem 'sqlite3'


# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'uglifier'
end

gem 'jquery-rails'

group :development, :test do
  gem 'rspec-rails'
end

group :test do
  gem 'capybara'
end

